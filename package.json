{
  "name": "proz",
  "version": "0.0.0-dev.1",
  "description": "TypeScript-powered HTTP RPC library for your own server and client",
  "license": "MIT",
  "author": "Timo MÃ¤mecke <maemecketimo@gmail.com> (https://github.com/timomeh)",
  "repository": "timomeh/proz",
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "test": "vitest --dir test",
    "publish-test": "vitest --dir test --watch false",
    "test-types": "tsd",
    "pretest-build": "del-cli dist example/.next && npm run build && (cd example && npm run build)",
    "test-build": "vitest scripts/app-bundle.test.ts --watch=false",
    "release": "np --no-cleanup --test-script=publish-test",
    "prepare": "npm run build",
    "build": "del-cli dist && tsc"
  },
  "sideEffects": false,
  "type": "module",
  "exports": "./dist/index.js",
  "types": "dist",
  "files": [
    "dist"
  ],
  "keywords": [
    "rpc",
    "api",
    "simple",
    "typescript",
    "typed",
    "client",
    "nextjs",
    "graphql",
    "rest"
  ],
  "devDependencies": {
    "del-cli": "^4.0.1",
    "got": "^12.1.0",
    "msw": "^0.39.2",
    "node-mocks-http": "^1.11.0",
    "np": "^7.6.1",
    "prettier": "^2.6.2",
    "tsd": "^0.20.0",
    "typescript": "4.7.1-rc",
    "vitest": "^0.12.6",
    "yup": "^0.32.11",
    "zod": "^3.17.3"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true
  },
  "tsd": {
    "directory": "./test"
  },
  "np": {
    "yarn": false
  }
}
